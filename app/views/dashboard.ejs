<script>
  var version = '0.0.1',
  pathname = location.pathname.replace(/\/[^/]+$/, ''),
  URL = pathname + (pathname.substr(-1) !== '/' ? '/' : ''),
  dojoConfig = {
    parseOnLoad: false,
    isDebug: false,
    async: true,
    cacheBust: 'v=' + version,
    packages: [
      { name: 'js', location: URL + 'js' },
    ],
    deps: [
      'dojo/domReady!'
    ],
    callback: function() { require(['js/config','js/main']);
    }
  };
</script>
